{
  "name": "lighthouse-keeper",
  "version": "1.0.0",
  "scripts": {
    "install:all": "run-p run-install:*",
    "run-install:consumer": "cd functions/consumer && npm install",
    "run-install:producer": "cd functions/producer && npm install",
    "run-install:runner": "cd functions/runner && npm install",
    "run-install:s3": "cd functions/integration/s3 && npm install",
    "run-install:spread_sheets": "cd functions/integration/spread_sheets && npm install",
    "build:all": "run-p run-build:*",
    "run-build:consumer": "cd functions/consumer && npm run build",
    "run-build:producer": "cd functions/producer && npm run build",
    "run-build:runner": "cd functions/runner && npm run build",
    "run-build:s3": "cd functions/integration/s3 && npm run build",
    "run-build:spread_sheets": "cd functions/integration/spread_sheets && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kentaro-m/lighthouse-keeper.git"
  },
  "author": "Kentaro Matsushita",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kentaro-m/lighthouse-keeper/issues"
  },
  "homepage": "https://github.com/kentaro-m/lighthouse-keeper#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
